				Height="198"
				Canvas.Left="71"
				Canvas.Top="71" Stroke="Black"/>
			<Line X1="170" Y1="0" X2="170" Y2="340" Stroke="Black" RenderTransformOrigin="1 0.5">
				<Line.RenderTransform>
					<RotateTransform Angle="9"></RotateTransform>
				</Line.RenderTransform>
			</Line>
			<Line X1="170" Y1="0" X2="170" Y2="340" Stroke="Black" RenderTransformOrigin="1 0.5">
