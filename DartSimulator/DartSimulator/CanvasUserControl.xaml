					<RotateTransform Angle="135"></RotateTransform>
				</Line.RenderTransform>
			</Line>
			<Line X1="170" Y1="0" X2="170" Y2="340" Stroke="Black" RenderTransformOrigin="1 0.5">
				<Line.RenderTransform>
					<RotateTransform Angle="153"></RotateTransform>
				</Line.RenderTransform>
			</Line>
			<Line X1="170" Y1="0" X2="170" Y2="340" Stroke="Black" RenderTransformOrigin="1 0.5">
				<Line.RenderTransform>
					<RotateTransform Angle="171"></RotateTransform>
				</Line.RenderTransform>
			</Line>
			<Ellipse
				Width="31.8"
				Height="31.8"
				Canvas.Left="154.1"
				Canvas.Top="154.1" Stroke="Black" Fill="Green"/>
